<Window x:Name="winMain" x:Class="WPFWinMgr.MainView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainView" Height="350" Width="525" Loaded="winMain_Loaded">
    <Grid x:Name="WinMgr">
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="182,7,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btn1" Content="Copy" HorizontalAlignment="Left" Margin="307,8,0,0" VerticalAlignment="Top" Width="75" Click="btn1_Click"/>
        <Button x:Name="bin2" Content="Fill" HorizontalAlignment="Left" Margin="10,8,0,0" VerticalAlignment="Top" Width="75" Click="bin2_Click">
        </Button>
        <Canvas x:Name="cv" HorizontalAlignment="Left" Height="193" Margin="73,69,0,0" VerticalAlignment="Top" Width="347"/>
        <ComboBox x:Name="winList" HorizontalAlignment="Left" Margin="387,8,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="winList_SelectionChanged"/>
        <Button Content="F11" HorizontalAlignment="Left" Margin="102,7,0,0" VerticalAlignment="Top" Width="25" Click="Button_Click"/>
    </Grid>
</Window>
